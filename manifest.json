{
  "description": "LesserDog",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/dynamic-theme",
  "manifest_version": 2,
  "name": "LesserDog",
  "permissions": [
    "theme"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["client.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55.0a2"
    }
  },
  "web_accessible_resources": [
    "lesserBody.png",
    "lesserHead.png"
  ]
}